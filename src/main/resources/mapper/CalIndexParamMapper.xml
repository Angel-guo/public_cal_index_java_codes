<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.catl.cal_index.mapper.calIndex.CalIndexParamMapper">
    <select id="getAllParentParam" resultType="com.catl.cal_index.entity.calIndex.CalIndexParam">
        select *
        from cal_index_param
        where pid = -1
    </select>
    <select id="getAllSonParam" resultType="com.catl.cal_index.entity.calIndex.CalIndexParam">
        select *
        from cal_index_param
        where pid != -1
    </select>
</mapper>
